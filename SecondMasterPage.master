            document.getElementById('banner').style.filter = 'none';
            // Hide the loader container when the page has loaded
            document.querySelector('.loader-container').style.display = 'none';
        });

        function changeTheme(color) {
            // Remove any existing theme classes
            document.body.classList.remove('red-theme', 'green-theme', 'blue-theme');

            // Add the appropriate theme class based on the color parameter
            if (color === 'red') {
                document.body.classList.add('red-theme');
                setThemeCookie('red');
            } else if (color === 'green') {
                document.body.classList.add('green-theme');
                setThemeCookie('green');
            } else if (color === 'blue') {
                document.body.classList.add('blue-theme');
                setThemeCookie('blue');
            }
        }

        function setThemeCookie(color) {
            document.cookie = "theme=" + color + "; path=/";
        }

        function getThemeCookie() {
            const name = "theme=";
            const decodedCookie = decodeURIComponent(document.cookie);
            const cookieArray = decodedCookie.split(';');
            for (let i = 0; i < cookieArray.length; i++) {
                let cookie = cookieArray[i];
                while (cookie.charAt(0) === ' ') {
                    cookie = cookie.substring(1);
                }
                if (cookie.indexOf(name) === 0) {
                    return cookie.substring(name.length, cookie.length);
                }
            }
            return "";
        }

        window.onload = function () {
            const theme = getThemeCookie();
            if (theme) {
                changeTheme(theme);
            }
        };
    </script>
